#include <windows.h>

#define IDD_DLGTEST 101
#define IDC_EDIT1 1000
#define IDC_EDIT2 1001
#define IDC_EDIT3 1003
#define Plus 1002
#define Minus 1004
#define Umn 1005
#define Del 1006
#define Exit 1007

IDD_DLGTEST DIALOG DISCARDABLE 0, 0, 200, 300
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION |
WS_SYSMENU
CAPTION "Calc3.0"
FONT 8, "MS Sans Serif"
BEGIN
    PUSHBUTTON "+", Plus, 120, 100, 70, 80, WS_VISIBLE | WS_CHILD | WS_BORDER
    PUSHBUTTON "-", Minus, 40, 100, 70, 80, WS_VISIBLE | WS_CHILD | WS_BORDER
    PUSHBUTTON "*", Umn, 40, 190, 70, 80, WS_VISIBLE | WS_CHILD | WS_BORDER
    PUSHBUTTON "/", Del, 120, 190, 70, 80, WS_VISIBLE | WS_CHILD | WS_BORDER
    PUSHBUTTON "EXIT", Exit, 8, 280, 30, 15, WS_VISIBLE | WS_CHILD | WS_BORDER
    EDITTEXT IDC_EDIT1, 5, 40, 90, 30, ES_AUTOHSCROLL
    EDITTEXT IDC_EDIT2, 105, 40, 90, 30, ES_AUTOHSCROLL
    EDITTEXT IDC_EDIT3, 5, 5, 190, 30, ES_AUTOHSCROLL
END